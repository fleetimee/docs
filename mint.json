{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/logo/email_logo.png",
    "light": "/logo/email_logo.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FF69B4",
    "light": "#FFB6C1",
    "dark": "#FF1493",
    "anchors": {
      "from": "#FF69B4",
      "to": "#FFB6C1"
    }
  },

  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "database"]
    },

    {
      "group": "Tables",
      "pages": [
        "tables/birthday-message",
        "tables/conversations",
        "tables/fcm-tokens",
        "tables/idol",
        "tables/idol-top"
      ],
      "icon": "table"
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  },
  "theme": "prism"
}
